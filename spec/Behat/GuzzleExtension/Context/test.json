{
    "name": "HTTP Bin",
    "operations": {
        "Get": {
            "httpMethod": "GET",
            "uri": "get",
            "summary": "Gets some JSON"
        },
        "GetLines": {
            "httpMethod": "GET",
            "uri": "stream/{count}",
            "summary": "Gets a response containing x lines",
            "parameters": {
                "count": {
                    "location": "uri",
                    "description": "Count of lines to return",
                    "required": true
                }
            }
        },
        "GetStatus": {
            "httpMethod": "GET",
            "uri": "status/{code}",
            "summary": "Gets a response with status code",
            "parameters": {
                "code": {
                    "location": "uri",
                    "description": "HTTP status code",
                    "required": true
                }
            }
        },
        "Get404": {
            "httpMethod": "GET",
            "uri": "get/404",
            "summary": "Gets a 404 response"
        },
        "GetRobots": {
            "httpMethod": "GET",
            "uri": "robots.txt",
            "summary": "Gets robots.txt"
        }
    }
}
